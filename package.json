{
  "name": "sasspect",
  "version": "1.5",
  "description": "SASS Aspect: The Foundation for Modern Websites ",
  "homepage": "https://sasspect.netlify.app/",
  "author": "drementer <klncarslanburak@gmail.com> (https://github.com/drementer)",
  "keywords": [ "scss" ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drementer/SASSpect.git"
  },
  "bugs": {
    "url": "https://github.com/drementer/SASSpect/issues"
  },
  "scripts": {
		"dev": "sass --watch src/scss/main.scss src/css/main.min.css --style expanded --error-css",
    "prod": "sass src/scss/main.scss src/css/main.min.css --style compressed --no-error-css",
    "doc": "sassdoc src/scss -d doc"
  },
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.53.0",
    "sassdoc": "^2.7.4"
  }
}
